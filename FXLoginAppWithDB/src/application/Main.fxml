<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <Label fx:id="isConnected" layoutX="113.0" prefHeight="44.0" prefWidth="236.0" text="Status:" textFill="#a12424">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtUserName" layoutX="140.0" layoutY="72.0" promptText="UserName">
         <font>
            <Font size="12.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" layoutX="140.0" layoutY="130.0" promptText="Password">
         <font>
            <Font size="12.0" />
         </font>
      </PasswordField>
      <Button fx:id="login" layoutX="189.0" layoutY="204.0" mnemonicParsing="false" onAction="#Login" text="Login">
         <font>
            <Font size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
